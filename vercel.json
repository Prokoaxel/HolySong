{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/mobile-v1",
      "destination": "/mobile-v1.html"
    },
    {
      "source": "/mobile-v1.html",
      "destination": "/mobile-v1.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
